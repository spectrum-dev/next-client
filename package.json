{
  "name": "next-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@chakra-ui/button": "1.5.1",
    "@chakra-ui/react": "1.7.3",
    "@chakra-ui/system": "1.8.3",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@react-financial-charts/annotations": "1.3.1",
    "@react-financial-charts/axes": "1.3.1",
    "@react-financial-charts/core": "1.3.1",
    "@react-financial-charts/scales": "1.3.1",
    "@react-financial-charts/series": "1.3.2",
    "@sentry/react": "6.16.1",
    "@sentry/tracing": "6.16.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/history": "4.7.9",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.15",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/react-select": "5.0.0",
    "axios": "0.24.0",
    "d3-format": "3.1.0",
    "d3-time-format": "4.1.0",
    "eslint": "7.32.0",
    "framer-motion": "4.1.17",
    "graphql": "16.2.0",
    "history": "4.10.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-data-table-component": "7.0.0-rc2",
    "react-datepicker": "4.5.0",
    "react-dom": "17.0.2",
    "react-dropdown": "1.9.2",
    "react-financial-charts": "1.3.2",
    "react-flow-renderer": "9.7.1",
    "react-focus-lock": "2.7.1",
    "react-google-login": "5.2.2",
    "react-icons": "4.3.1",
    "react-id-generator": "3.0.2",
    "react-reflex": "4.0.3",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-select": "5.2.1",
    "styled-components": "5.3.3",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "typescript": "4.5.4",
    "web-vitals": "2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check-code-lint": "eslint src --ext .jsx,.js,.tsx,.ts --max-warnings 0",
    "fix-code-lint": "eslint src --ext .jsx,.js,.tsx,.ts --fix --max-warnings 0",
    "check-style-lint": "stylelint 'src/**/*.css'",
    "fix-style-lint": "stylelint --fix 'src/**/*.css'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "eslint-config-airbnb-typescript": "14.0.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4"
  },
  "lint-staged": {
    "src/**/*.{jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier -write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelin --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb-typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-console": "off",
      "import/prefer-default-export": "off",
      "react/jsx-props-no-spreading": "off",
      "react/no-unused-prop-types": "off",
      "react/require-default-props": "off",
      "no-restricted-syntax": "off"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-financial-charts|@react-financial-charts|d3-(array|format|time-format)|react-datepicker|react-dropdown)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
