{
  "name": "next-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/button": "^1.4.1",
    "@chakra-ui/react": "^1.0.0",
    "@chakra-ui/system": "^1.7.1",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@react-financial-charts/axes": "^1.3.1",
    "@react-financial-charts/core": "^1.3.1",
    "@react-financial-charts/scales": "^1.3.1",
    "@react-financial-charts/series": "^1.3.1",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/history": "^4.7.9",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-select": "^4.0.17",
    "axios": "^0.21.1",
    "eslint": "^7.30.0",
    "framer-motion": "^4.0.0",
    "history": "^4.10.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-dropdown": "^1.9.2",
    "react-financial-charts": "^1.3.1",
    "react-flow-renderer": "^9.6.4",
    "react-focus-lock": "^2.5.0",
    "react-google-login": "^5.2.2",
    "react-icons": "^3.0.0",
    "react-id-generator": "^3.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.5",
    "web-vitals": "^0.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check-code-lint": "eslint src --ext .jsx,.js,.tsx,.ts --max-warnings 0",
    "fix-code-lint": "eslint src --ext .jsx,.js,.tsx,.ts --fix --max-warnings 0",
    "check-style-lint": "stylelint 'src/**/*.css'",
    "fix-style-lint": "stylelint --fix 'src/**/*.css'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier -write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelin --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb-typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-console": "off",
      "import/prefer-default-export": "off",
      "react/jsx-props-no-spreading": "off",
      "react/no-unused-prop-types": "off",
      "react/require-default-props": "off",
      "no-restricted-syntax": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
